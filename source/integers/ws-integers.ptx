	<worksheet xml:id="worksheet-signed-practice">
		<title>Worksheet: Arithmetic Practice</title>
		<page>
			<exercisegroup cols="2">
				<title>Practice with Units.</title>
				<introduction>
					<p>
						Perform the indicated operation and write your answer with the proper units.
					</p>
				</introduction>
				<exercise workspace="0.4in">
					<statement>
					<p><m>$301 + $452</m></p>
					</statement>
				</exercise>
				<exercise workspace="0.4in">
					<statement>
					<p><m>835 \text{ miles } + 406 \text{ miles }</m></p>
					</statement>
